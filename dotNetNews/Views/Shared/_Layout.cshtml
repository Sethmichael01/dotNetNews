<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - dotNetNews</title>
    <link href="~/courseware/assets/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/courseware/assets/css/font-awesome.min.css" rel="stylesheet" />
    <link href="~/courseware/assets/css/magnific-popup.css" rel="stylesheet" />
    <link href="~/courseware/assets/css/owl.carousel.min.css" rel="stylesheet" />
    <link href="~/courseware/assets/css/responsive.css" rel="stylesheet" />
    <link href="~/courseware/assets/css/selectric.css" rel="stylesheet" />
    <link href="~/courseware/assets/css/simple-line-icons.css" rel="stylesheet" />
    <link href="~/courseware/assets/css/style.css" rel="stylesheet" />
    <link href="~/courseware/assets/css/timeTo.css" rel="stylesheet" />
    <link href="~/courseware/assets/css/woocommerce.css" rel="stylesheet" />
    <script src="~/courseware/assets/js/jquery.selectric.js"></script>
    <script src="~/courseware/assets/js/jquery-3.3.1.min.js"></script>
    <script src="~/courseware/assets/js/jquery-ui.min.js"></script>
    <script src="~/courseware/assets/js/main.js"></script>
    <script src="~/courseware/assets/js/plugins.js"></script>
    <script src="//cdn.ckeditor.com/4.13.1/full/ckeditor.js"></script>
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
            <div class="container">
                <a class="navbar-brand" asp-area="" asp-controller="dotNetNewsModels" asp-action="Index">dotNetNews</a>
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex flex-sm-row-reverse">
                    <partial name="_LoginPartial" />
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="dotNetNewsModels" asp-action="Index">Home</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2020 - dotNetNews - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>
